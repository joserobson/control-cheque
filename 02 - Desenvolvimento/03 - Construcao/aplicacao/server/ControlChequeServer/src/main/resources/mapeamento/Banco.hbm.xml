<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="br.com.controlcheque.server.model.Banco" table="tb_banco">
    <id column="idbanco" name="id" type="string">
      <generator class="uuid"/>
    </id>
    <property column="codigoBanco" name="codigo"/>
    <property column="descricaoBanco" name="descricao"/>
  </class>
</hibernate-mapping>
